{
  "name": "demoan",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build demoanapp && nest build demoanappfunc",
    "install-build-client": "cd apps/demoanapp/client  && npm install && npm run build",
    "build-prod": "npm install && npm run build && npm run install-build-client",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start-client-watch": "cd apps/demoanapp/client && npm run start",
    "start-func-watch": "nest start --watch demoanappfunc && cd dist/apps/demoanappfunc && func start",
    "start": "npm-run-all --parallel -l start-client-watch start:dev start-func-watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "generate-api": "npx openapi-generator-cli generate -i \"swagger.json\" -o \"apps/demoanapp/client/src/generated-api -g typescript-axios --artifact-version 1.0.0-SNAPSHOT --skip-validate-spec",
    "start:azure": "npm run build && func host start"
  },
  "dependencies": {
    "@azure/cosmos": "^3.14.1",
    "@azure/functions": "^2.0.0",
    "@azure/storage-blob": "12.6.0",
    "@nestjs/common": "^8.1.2",
    "@nestjs/core": "^8.1.2",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.1.2",
    "@nestjs/swagger": "5.0.9",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "copy-webpack-plugin": "^9.0.1",
    "demofile": "^2.1.0",
    "lodash": "^4.17.21",
    "passport": "^0.4.1",
    "passport-azure-ad": "^4.3.1-beta.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.2.0",
    "swagger-ui-express": "4.1.6",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.4",
    "@nestjs/schematics": "8.0.2",
    "@nestjs/testing": "^8.1.2",
    "@openapitools/openapi-generator-cli": "^2.4.5",
    "@types/express": "4.17.13",
    "@types/lodash": "^4.14.175",
    "@types/multer": "1.4.7",
    "@types/node": "14.17.6",
    "@types/passport-azure-ad": "^4.3.1",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "jest": "27.0.6",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "supertest": "6.1.4",
    "swagger-react-generator": "^2.0.0",
    "ts-jest": "27.0.4",
    "ts-loader": "9.2.4",
    "ts-node": "10.1.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  }
}
