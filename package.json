{
  "name": "demoan",
  "version": "1.0.0",
  "description": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "install-build-client": "cd apps/demoanapp/client  && npm install && npm run build",
    "build-prod": "npm install && npm run build && npm run install-build-client",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start-client-watch": "cd apps/demoanapp/client && npm run start",
    "start-func-watch": "cd functions && npm run start",
    "start": "npm-run-all --parallel -l start-client-watch start:dev start-func-watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "generate-api": "node node_modules/swagger-react-generator -I server/swagger.json -O client/src/generated-api/index.ts -T \"ts\"",
    "start:azure": "npm run build && func host start"
  },
  "dependencies": {
    "@azure/functions": "^1.0.3",
    "@azure/storage-blob": "12.6.0",
    "@nestjs/common": "8.0.4",
    "@nestjs/core": "8.0.4",
    "@nestjs/platform-express": "8.0.4",
    "@nestjs/swagger": "5.0.9",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "demofile": "^2.1.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.2.0",
    "swagger-ui-express": "4.1.6"
  },
  "devDependencies": {
    "@nestjs/cli": "8.0.2",
    "@nestjs/schematics": "8.0.2",
    "@nestjs/testing": "8.0.4",
    "@types/express": "4.17.13",
    "@types/multer": "1.4.7",
    "@types/node": "14.17.6",
    "@types/supertest": "2.0.11",
    "jest": "27.0.6",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.2",
    "supertest": "6.1.4",
    "ts-jest": "27.0.4",
    "ts-loader": "9.2.4",
    "ts-node": "10.1.0",
    "tsconfig-paths": "3.10.1",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "eslint": "7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "swagger-react-generator": "^2.0.0",
    "typescript": "4.3.5"
  }
}
