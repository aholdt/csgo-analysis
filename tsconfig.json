{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "paths": {
      "@app/demoanapp": ["apps/demoanapp/src"],
      "@app/demoanapp/*": ["apps/demoanapp/src/*"],
      "@app/demoparser": ["libs/demoparser/src"],
      "@app/demoparser/*": ["libs/demoparser/src/*"],
      "@app/blob-storage": ["libs/blob-storage/src"],
      "@app/blob-storage/*": ["libs/blob-storage/src/*"]
    }
  },
  "include": ["libs/**/*/src", "apps/**/*/src", "functions/**/*"]
}
